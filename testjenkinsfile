pipeline {
    agent any

    stages {
        stage('Dep install') {
            agent {
                dockerContainer {
                    image 'node:14'
                }
            }
            steps {
                sh 'apt update'
                sh 'apt install sudo -y'
                sh 'sudo chown -R 995:993 "/ .npm"'
                sh 'npm install'
            }
        }
    }
}
